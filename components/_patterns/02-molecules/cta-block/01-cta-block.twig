<div class="cta-block">
  {% include "@atoms/02-text/00-headings/_heading.twig" with {
      heading_level: 3,
      heading: cta_heading
  }%}
  <div class="flex-ns">
    <div class="cta-content">
      {% if pl %}
        {{ cta_content|raw }}
      {% else %}
        {{ cta_content }}
      {% endif %}

    </div>
    <div class="cta-link">
      {% include "@atoms/01-links/link/link.twig" with {
        link_url: cta_link,
        link_content: cta_link_title,
        link_base_class:"button btn-green",

      } %}
    </div>
  </div>
</div>